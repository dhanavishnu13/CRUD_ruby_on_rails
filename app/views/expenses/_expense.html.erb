<div id="<%= dom_id expense %>">
  <p>
    <strong>Payee name:</strong>
    <%= expense.payee_name %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= expense.category %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= expense.description %>
  </p>

  <p>
    <strong>Amount:</strong>
    <%= expense.amount %>
